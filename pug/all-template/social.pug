mixin social(v,i,tw,f,y,g)
  .social
    if v == 'v'
      a(href="#")
        img(src="../images/social/vk-black.svg", alt="")
    if v == 'vw'
      a(href="#").social-ico-white
        img(src="../images/social/vk-white.svg", alt="")
    if i == 'i'
      a(href="#")
        img(src="../images/social/instagram-black.svg", alt="")
    if i == 'iw'
      a(href="#").social-ico-white
        img(src="../images/social/instagram-white.svg", alt="")
    if tw == 'tw'
      a(href="#")
        img(src="../images/social/twitter-black.svg", alt="")
    if tw == 'tww'
      a(href="#").social-ico-white
        img(src="../images/social/twitter-white.svg", alt="")
    if f == 'f'
      a(href="#")
        img(src="../images/social/facebook-black.svg", alt="")
    if f == 'fw'
      a(href="#").social-ico-white
        img(src="../images/social/facebook-white.svg", alt="")
    if y == 'y'
      a(href="#")
        img(src="../images/social/youtube-black.svg", alt="")
    if y == 'yw'
      a(href="#").social-ico-white
        img(src="../images/social/youtube-white.svg", alt="")
    if g == 'g'
      a(href="#")
        img(src="../images/social/google-black.svg", alt="")
    if g == 'gw'
      a(href="#").social-ico-white
        img(src="../images/social/google-white.svg", alt="")
        